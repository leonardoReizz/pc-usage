generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model PCUsage {
  id          Int      @id @default(autoincrement())
  coresUage   Float
  totalCores  Float
  memoryUsage Float
  totalMemory Float
  diskUsage   Float
  totalDisk   Float
  updatedAt   DateTime @updatedAt
  createdAt   DateTime @default(now())
}
